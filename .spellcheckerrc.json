{
  "files": [
    "./public/*.html",
    "./public/*.txt",
    "./public/*.json",
    "./src/**/*.html",
    "./src/**/*.json",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "dictionaries": [
    "dictionary.txt"
  ],
  "ignore": [
    "[0-9]+.*",
    "[a-fA-F0-9]+"
  ],
  "plugins": [
    "spell",
    "indefinite-article",
    "repeated-words",
    "syntax-mentions",
    "syntax-urls"
  ],
  "reports": [
    "./reports/spellcheck.json"
  ],
  "quiet": true
}
